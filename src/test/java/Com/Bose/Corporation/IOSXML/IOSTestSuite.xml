<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name = " Ios Regression Suite">

    <listeners>
        <listener class-name="Com.Bose.Corporation.Utilities.Listener.TestListener" />
        <listener class-name="Com.Bose.Corporation.Utilities.Listener.AnnotationTransformer" />
    </listeners>

    <test  name= "OnBoardingTest" parallel="false" preserve-order="true">
        <classes>
            <class name="Com.Bose.Corporation.TestSuite.Onboarding.OnboardingTest">
                <methods>
                    <exclude name="OB_TC_C116884"></exclude>
                    <exclude name="OB_TC_C116906"></exclude>
                </methods>
            </class>
        </classes>
    </test>

    <test name="HomepageTest" parallel="false" preserve-order="true">
        <classes>
            <class name="Com.Bose.Corporation.TestSuite.Home.HomePageTest"></class>
        </classes>
    </test>

    <test name="AboutPageTest" parallel="false" preserve-order="true">
        <classes>
            <class name="Com.Bose.Corporation.TestSuite.About.AboutPageTest"></class>
        </classes>
    </test>

    <test name="SettingsPageTest" parallel="false" preserve-order="true">
        <classes>
            <class name="Com.Bose.Corporation.TestSuite.Settings.SettingsPageTest"></class>
        </classes>
    </test>
</suite>
